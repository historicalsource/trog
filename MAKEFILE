#
.SUFFIXES : .out .asm .obj
#
OBJS =	TROGRAM.OBJ TROG.OBJ TROGUTIL.OBJ TROGPALL.OBJ TROGBGND.OBJ \
	IMGPAL.OBJ IMGTBL.OBJ BGNDTBL.OBJ BGNDPAL.OBJ TROGPROC.OBJ \
	TROGDISP.OBJ TROGSND.OBJ TROGTV.OBJ TROGPOST.OBJ TROGBAR.OBJ \
	TROGDINO.OBJ TROGMISC.OBJ TROGTEXT.OBJ TROGCOL.OBJ TROGMAPS.OBJ \
	TROGDDAT.OBJ TROGTROG.OBJ TROGTDAT.OBJ TROGENEM.OBJ TROGEGGS.OBJ \
	TROGWHL.OBJ TROGDRON.OBJ TROGSPRG.OBJ TROGWAVE.OBJ \
	TROGTREX.OBJ TROGDIN2.OBJ

KOBJS = TROGMENU.OBJ TROGHSTD.OBJ TROGJUNK.OBJ SYSFONT.OBJ TROGDUTL.OBJ \
	TROGDTST.OBJ TROGCOIN.OBJ TROGSCOR.OBJ TROGATT.OBJ \
	TROGADJ.OBJ TROGPRNT.OBJ TROGCAVE.OBJ TROGFADE.OBJ TROGWORD.OBJ \
	TROGCAT.OBJ TROGHAUS.OBJ

.asm.obj:
        @ ECHO *----assembling----$*.ASM(0)
        GSPA $* $*.OBJ f:\video\trog\$*.LST -H -L -v10
	CRNCHLST f:\video\trog\$*.LST

f:\video\trog\TROG.OUT: $(OBJS) f:\video\trog\TROGKURT.RLO
	GLNKL TROG

f:\video\trog\TROGKURT.RLO: $(KOBJS)
	GLNK TROGKURT

$(OBJS): $*.ASM
$(KOBJS): $*.ASM
TROGPROC.OBJ: MPROCEQU.ASM
TROGDISP.OBJ: DISPEQU.ASM SYSINC.ASM
TROGMISC.OBJ: DISPEQU.ASM SYSINC.ASM TROGAEQU.ASM
TROGDDAT.OBJ: TROGDDAT.TBL
TROGSCOR.OBJ: TROGSCOR.TBL SYSINC.ASM
TROGTDAT.OBJ: TROGTDAT.TBL
TROGDINO.OBJ: TROGAEQU.ASM SYSINC.ASM
TROGTROG.OBJ: TROGAEQU.ASM
TROGTEXT.OBJ: STRING.H TROGTEXT.TBL SYSINC.ASM
TROGPALL.OBJ: SYSINC.ASM
TROGUTIL.OBJ: SYSINC.ASM
TROG.OBJ: SYSINC.ASM
TROGENEM.OBJ: TROGENEM.TBL
TROGCOIN.OBJ: SYSINC.ASM
TROGDTST.OBJ: TROGCHIP.ASM SYSINC.ASM
TROGDUTL.OBJ: SYSINC.ASM
TROGJUNK.OBJ: SYSINC.ASM
TROGEGGS.OBJ: TROGAEQU.ASM
TROGWHL.OBJ: TROGAEQU.ASM TROGWHL.TBL
TROGPRNT.OBJ:	SYSINC.ASM
TROGDRON.OBJ: TROGAEQU.ASM
TROGADJ.OBJ: SYSINC.ASM
TROGSPRG.OBJ: TROGSPRG.TBL
TROGCAVE.OBJ: TROGCAVE.TBL SYSINC.ASM
TROGTREX.OBJ: TROGTREX.TBL TROGAEQU.ASM
TROGMENU.OBJ: SYSINC.ASM
